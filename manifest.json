{
    "manifest_version": 3,
    "name": "Golyatsec Browser Guard",
    "version": "1.0",
    "description": "Blocks phishing domains from being accessed in the browser.",
    "permissions": ["storage", "scripting", "declarativeNetRequest", "declarativeNetRequestFeedback", "activeTab", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  